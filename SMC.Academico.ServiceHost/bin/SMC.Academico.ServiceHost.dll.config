    <client>
      <!-- Segurança para SMCAuthorize -->
      <endpoint name="SMC.Framework.Security.ISMCAuthorizationService" address="net.tcp://web-desenvolvimento.pucminas.br:808/Seguranca/v4/AutorizacaoService.svc" binding="netTcpBinding" bindingConfiguration="SMCNetTcpLarge" contract="SMC.Framework.Security.ISMCAuthorizationService" />
      <!-- Serviço para Filtro de dados (SEGURANCA) -->
      <endpoint name="SMC.Framework.DataFilters.ISMCDataFilterService" address="net.pipe://localhost/Dev/Seguranca/v4/DataFilterService.svc" binding="netNamedPipeBinding" bindingConfiguration="SMCNetPipeLarge" contract="SMC.Framework.DataFilters.ISMCDataFilterService" />
