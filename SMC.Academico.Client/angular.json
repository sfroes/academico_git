{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "administrativo": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/smc-sga-administrativo",
      "sourceRoot": "projects/smc-sga-administrativo/src",
      "prefix": "sga",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser-esbuild",
          "options": {
            "outputPath": "dist/administrativo",
            "index": "projects/smc-sga-administrativo/src/index.html",
            "main": "projects/smc-sga-administrativo/src/main.ts",
            "polyfills": ["zone.js"],
            "tsConfig": "projects/smc-sga-administrativo/tsconfig.app.json",
            "baseHref": "/Dev/SGA.Administrativo/App/",
            "assets": [
              "projects/smc-sga-administrativo/src/favicon.ico",
              "projects/smc-sga-administrativo/src/assets",
              {
                "glob": "**/*",
                "input": "SMC.Academico.Recursos/Content/Fonts",
                "output": "Content/Fonts"
              },
              {
                "glob": "**/*",
                "input": "../Framework4.2/SMC.Recursos",
                "output": "Recursos/4.0"
              },
              {
                "glob": "**/*",
                "input": "../SMC.Academico.Recursos",
                "output": "Recursos/Academico/4.0"
              }
            ],
            "styles": [
              "node_modules/angular-calendar/css/angular-calendar.css",
              "node_modules/@po-ui/style/css/po-theme-default-variables.min.css",
              "node_modules/@po-ui/style/css/po-theme-core.min.css",
              "node_modules/primeicons/primeicons.css",
              "projects/smc-sga-administrativo/src/styles.scss"
            ],
            "scripts": [
              "projects/smc-sga-administrativo/src/assets/recursos/node_modules/jquery/dist/jquery.js",
              "projects/smc-sga-administrativo/src/assets/recursos/scripts/smc-developwarning.js"
            ],
            "allowedCommonJsDependencies": [
              "angular-calendar/date-adapters/date-fns",
              "calendar-utils/date-adapters/date-fns",
              "dom-set",
              "dom-plane",
              "@mattlewis92/dom-autoscroller"
            ],
            "optimization": {
              "scripts": false,
              "styles": {
                "minify": false,
                "inlineCritical": false
              },
              "fonts": false
            }
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/smc-sga-administrativo/src/environments/environment.ts",
                  "with": "projects/smc-sga-administrativo/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "buildTarget": "administrativo:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "administrativo:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "administrativo:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/smc-sga-administrativo/src/test.ts",
            "polyfills": "projects/smc-sga-administrativo/src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": ["projects/smc-sga-administrativo/src/favicon.ico", "projects/smc-sga-administrativo/src/assets"],
            "styles": [
              "node_modules/angular-calendar/css/angular-calendar.css",
              "projects/smc-sga-administrativo/src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["tsconfig.app.json", "tsconfig.spec.json", "e2e/tsconfig.json"],
            "exclude": ["**/node_modules/**"]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/smc-sga-administrativo/e2e/protractor.conf.js",
            "devServerTarget": "administrativo:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "administrativo:serve:production"
            }
          }
        }
      }
    },
