@model List<string>
@if (Model.Count > 0)
{
    using (Smc.Div().Size(SMCSize.Grid24_24).CssClass("smc-mensagem-informativa smc-mensagem").Begin())
    {

        <p>
            @Smc.GetUIResource("MSG_Informativa")
            @foreach (var item in Model)
            {

                <ul class="smc-display-collection">

                    <li>@item</li>
                </ul>
            }
        </p>
    }
}


