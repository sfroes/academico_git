@using SMC.Academico.UI.Mvc.Areas.CUR.Lookups;
@using SMC.Framework;
@using SMC.Framework.Extensions;
@using System.Collections;
@using System.Linq;
@model OfertaMatrizCurricularLookupViewModel

@using (Smc.Div().CssClass("smc-list-item smc-list-cabecalho smc-listdetailed-conteudo-header").Size(SMCSize.Grid24_24).Begin())
{

    using (Smc.Div().CssClass("smc-listdetailed-item").Size(SMCSize.Grid24_24).Begin())
    {
        @Smc.DisplayFor(m => m.DescricaoMatrizCurricular).CssClass("smc-listdetailed-conteudo-titulo").HideLabel().Size(SMCSize.Grid12_24)
        @Smc.DisplayFor(m => m.DescricaoComplementarMatrizCurricular).CssClass("smc-listdetailed-conteudo-titulo").HideLabel().Size(SMCSize.Grid12_24)
    }
}