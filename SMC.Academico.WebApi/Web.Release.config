<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform" xmlns:asmv1="urn:schemas-microsoft-com:asm.v1">
  <smc.rest>
    <apis xdt:Transform="Replace">
      <api name="CargaIngressante" url="http://localhost/Academico.WebAPI/api/CargaIngressanteIndividual" />
	  <api name="CurriculoGAD" url="http://localhost/AssinaturaDigital.WebApi/api/Curriculo" />
    </apis>
  </smc.rest>
  <system.web>
    <compilation xdt:Transform="RemoveAttributes(debug)" />
  </system.web>

  <system.serviceModel xdt:Transform="Replace">
