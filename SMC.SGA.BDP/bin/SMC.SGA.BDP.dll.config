  <system.net>
    <defaultProxy useDefaultCredentials="true">
      <proxy proxyaddress="http://proxy.pucminas.br" autoDetect="True" usesystemdefault="True" bypassonlocal="True" />
      <bypasslist>
        <add address="^([a-z0-9-]+\.)*pucminas\.br$" />
      </bypasslist>
    </defaultProxy>
  </system.net>
  
  <appSettings>
    <!--mvc4-->
    <add key="webpages:Version" value="3.0.0.0" />
    <add key="webpages:Enabled" value="false" />
    <add key="PreserveLoginUrl" value="true" />
    <add key="ClientValidationEnabled" value="true" />
    <add key="UnobtrusiveJavaScriptEnabled" value="true" />
    <!--habilita o fake-->
    <add key="DefaultFake" value="false" />
    <add key="EFContext" value="SMC.Academico.EntityRepository.AcademicoContext" />
  </appSettings>
  <smc.ioc type="SMC.Framework.Unity.SMCUnityContainer, SMC.Framework.Unity">
